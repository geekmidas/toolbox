{
  "search.exclude": {
    "**/.sst": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "cSpell.words": [
    "citext",
    "dbdata",
    "envkit",
    "fuzzystrmatch",
    "healthcheck",
    "Hermod",
    "hoverkraft",
    "Insertable",
    "isready",
    "kysely",
    "openauth",
    "openauthjs",
    "ossp",
    "paysafe",
    "pgcrypto",
    "presigner",
    "testkit",
    "trgm",
    "tsdown",
    "uniqby",
    "uuidv",
    "valibot",
    "wagerwars",
    "wiseguylegion"
  ],
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
