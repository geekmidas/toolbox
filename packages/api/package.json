{
  "name": "@geekmidas/api",
  "version": "0.0.26",
  "private": false,
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/exports/*.mjs",
      "require": "./dist/exports/*.cjs",
      "types": "./src/exports/*.ts"
    },
    "./errors": {
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.cjs",
      "types": "./src/errors.ts"
    },
    "./services": {
      "import": "./dist/services.mjs",
      "require": "./dist/services.cjs",
      "types": "./src/services.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs",
      "types": "./src/client/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geekmidas/toolbox.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@standard-schema/spec": "~1.0.0",
    "lodash.get": "~4.4.2",
    "lodash.merge": "~4.6.2",
    "lodash.set": "~4.3.2",
    "lodash.pick": "~4.4.0",
    "lodash.uniqby": "~4.7.0",
    "openapi-types": "~12.1.3",
    "openapi-typescript": "~7.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/react": "~16.3.0",
    "@testing-library/react-hooks": "~8.0.1",
    "@types/lodash.get": "~4.4.9",
    "@types/lodash.merge": "~4.6.9",
    "@types/lodash.set": "~4.3.9",
    "@types/lodash.pick": "~4.4.9",
    "@types/lodash.uniqby": "~4.7.9",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "fast-glob": "~3.3.3",
    "jsdom": "~26.1.0",
    "react": "~19.1.0",
    "react-dom": "~19.1.0"
  },
  "scripts": {
    "ts": "tsc --noEmit --skipLibCheck src/**/*.ts"
  },
  "peerDependencies": {
    "@geekmidas/envkit": "workspace:*",
    "@middy/core": ">=6.3.1",
    "@middy/http-header-normalizer": ">=6.3.1",
    "@middy/http-json-body-parser": ">=6.3.1",
    "@tanstack/react-query": ">=5.0.0",
    "@types/aws-lambda": ">=8.10.92",
    "@valibot/to-json-schema": ">=1.3.0",
    "hono": "~4.8.2",
    "hono-openapi": "~0.4.8",
    "zod": ">=3.25.67"
  }
}
