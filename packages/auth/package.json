{
	"name": "@geekmidas/auth",
	"version": "0.2.0",
	"description": "JWT and OIDC authentication with token verification, Hono middleware, and Lambda authorizers",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./server": {
			"types": "./dist/server.d.ts",
			"import": "./dist/server.mjs",
			"require": "./dist/server.cjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.cjs"
		},
		"./jwt": {
			"types": "./dist/jwt.d.ts",
			"import": "./dist/jwt.mjs",
			"require": "./dist/jwt.cjs"
		},
		"./hono/jwt": {
			"types": "./dist/hono/jwt.d.ts",
			"import": "./dist/hono/jwt.mjs",
			"require": "./dist/hono/jwt.cjs"
		},
		"./lambda/jwt": {
			"types": "./dist/lambda/jwt.d.ts",
			"import": "./dist/lambda/jwt.mjs",
			"require": "./dist/lambda/jwt.cjs"
		},
		"./oidc": {
			"types": "./dist/oidc.d.ts",
			"import": "./dist/oidc.mjs",
			"require": "./dist/oidc.cjs"
		},
		"./hono/oidc": {
			"types": "./dist/hono/oidc.d.ts",
			"import": "./dist/hono/oidc.mjs",
			"require": "./dist/hono/oidc.cjs"
		},
		"./lambda/oidc": {
			"types": "./dist/lambda/oidc.d.ts",
			"import": "./dist/lambda/oidc.mjs",
			"require": "./dist/lambda/oidc.cjs"
		}
	},
	"scripts": {
		"play": "tsx src/playground.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/geekmidas/toolbox"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"dependencies": {
		"@openauthjs/openauth": "~0.4.3",
		"jose": "~5.10.0",
		"jsonwebtoken": "^9.0.2",
		"@types/ms": "~2.1.0"
	},
	"devDependencies": {
		"@geekmidas/cache": "workspace:^",
		"@types/jsonwebtoken": "^9.0.5"
	},
	"peerDependencies": {
		"@geekmidas/cache": "workspace:^",
		"@types/aws-lambda": ">=8.10.92",
		"hono": ">=4.0.0"
	},
	"peerDependenciesMeta": {
		"@geekmidas/cache": {
			"optional": true
		},
		"@types/aws-lambda": {
			"optional": true
		},
		"hono": {
			"optional": true
		}
	}
}
