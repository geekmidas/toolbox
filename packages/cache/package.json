{
	"name": "@geekmidas/cache",
	"version": "0.2.0",
	"description": "Unified cache interface with multiple storage implementations",
	"private": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./memory": {
			"types": "./dist/memory.d.ts",
			"import": "./dist/memory.mjs",
			"require": "./dist/memory.cjs"
		},
		"./upstash": {
			"types": "./dist/upstash.d.ts",
			"import": "./dist/upstash.mjs",
			"require": "./dist/upstash.cjs"
		},
		"./expo": {
			"types": "./dist/expo.d.ts",
			"import": "./dist/expo.mjs",
			"require": "./dist/expo.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/geekmidas/toolbox"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"dependencies": {
		"date-fns": "~4.1.0"
	},
	"devDependencies": {},
	"peerDependencies": {
		"@upstash/redis": "~1.35.1",
		"expo-secure-store": "^14.2.3"
	}
}
