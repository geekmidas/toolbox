{
  "name": "@geekmidas/cache",
  "version": "0.0.5",
  "description": "Type-safe email client with SMTP support and React templates",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./memory": {
      "types": "./dist/memory.d.ts",
      "import": "./dist/memory.mjs",
      "require": "./dist/memory.cjs"
    },
    "./upstash": {
      "types": "./dist/upstash.d.ts",
      "import": "./dist/upstash.mjs",
      "require": "./dist/upstash.cjs"
    },
    "./expo": {
      "types": "./dist/expo.d.ts",
      "import": "./dist/expo.mjs",
      "require": "./dist/expo.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "date-fns": "~4.1.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@upstash/redis": "~1.35.1",
    "expo-secure-store": "^14.2.3"
  }
}
