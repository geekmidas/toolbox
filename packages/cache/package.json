{
  "name": "@geekmidas/cache",
  "version": "0.0.3",
  "description": "Type-safe email client with SMTP support and React templates",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./memory": {
      "import": "./dist/memory.mjs",
      "require": "./dist/memory.cjs",
      "types": "./src/memory.ts"
    },
    "./upstash": {
      "import": "./dist/upstash.mjs",
      "require": "./dist/upstash.cjs",
      "types": "./src/upstash.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {
    "@upstash/redis": "~1.35.1"
  }
}
