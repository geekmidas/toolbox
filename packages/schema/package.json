{
	"name": "@geekmidas/schema",
	"version": "1.0.0",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./conversion": {
			"types": "./dist/conversion.d.ts",
			"import": "./dist/conversion.mjs",
			"require": "./dist/conversion.cjs"
		},
		"./openapi": {
			"types": "./dist/openapi.d.ts",
			"import": "./dist/openapi.mjs",
			"require": "./dist/openapi.cjs"
		},
		"./parser": {
			"types": "./dist/parser.d.ts",
			"import": "./dist/parser.mjs",
			"require": "./dist/parser.cjs"
		}
	},
	"scripts": {
		"ts": "tsc --noEmit --skipLibCheck src/**/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/geekmidas/toolbox"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"dependencies": {
		"openapi-types": "~12.1.3",
		"@standard-schema/spec": "~1.0.0"
	},
	"devDependencies": {},
	"peerDependencies": {
		"@valibot/to-json-schema": ">=1.3.0",
		"zod-to-json-schema": "~3.25.1",
		"zod": "~4.1.13"
	}
}
