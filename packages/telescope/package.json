			"require": "./dist/instrumentation/hono.cjs"
		},
		"./core": {
			"types": "./dist/core.d.ts",
			"import": "./dist/core.mjs",
			"require": "./dist/core.cjs"
		},
		"./lambda": {
			"types": "./dist/adapters/lambda.d.ts",
			"import": "./dist/adapters/lambda.mjs",
			"require": "./dist/adapters/lambda.cjs"
		}
